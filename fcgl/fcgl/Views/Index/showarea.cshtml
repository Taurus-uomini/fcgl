@{
    Layout = null;
}

<!DOCTYPE html>
@using fcgl.Models
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>hello</title>
</head>
<body>
    <div>
        @foreach(ProvincesModels pm in ViewBag.plist)
        {
            @Html.Label("insert into ProvincesModels(provinceid,province) values('" + pm.provinceid + "','" + pm.province + "');")<br />
        }
        <br />
        <br />

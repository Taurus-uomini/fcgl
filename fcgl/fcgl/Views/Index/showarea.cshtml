        }
        <br />
        <br />
        @foreach (AreasModels am in ViewBag.alist)
        {
            @Html.Label("insert into AreasModels(areaid,area,cityid) values('" + am.areaid + "','" + am.area + "','" + am.cityid + "');")<br />
        }
